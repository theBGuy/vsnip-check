{
  "name": "vsnip-check",
  "displayName": "vsnip-check",
  "description": "Syntax highlighting and error reporting for .nip pickit files",
  "version": "1.1.1",
  "publisher": "theBGuy",
  "repository": "https://github.com/theBGuy/vsnip-check",
  "engines": {
    "vscode": "^1.85.5"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nip",
        "aliases": [
          "Nip",
          "nip"
        ],
        "extensions": [
          ".nip"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nip",
        "scopeName": "source.nip",
        "path": "./syntaxes/nip.tmLanguage.json"
      }
    ]
  },
  "main": "./out/diagnostics.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ."
  },
  "activationEvents": [
    "onDidOpenTextDocument",
    "onDidSaveTextDocument",
    "onDidChangeActiveTextEditor",
    "onDidOpenTextDocument",
    "onStartupFinished",
    "onDidCloseTextDocument"
  ],
  "devDependencies": {
    "@types/node": "^20.11.13",
    "@types/vscode": "^1.85.0",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  }
}
